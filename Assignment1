#include<stdio.h>

void delete(int arr[], int pos, int size);


int main()

{

int arr[20],n,pos,I,ele;

    printf("enter n");

    scanf("%d",&n);

    printf("enter array elements");

    for(I=0;I<n;I++)

    scanf("%d",&arr[I]);

    printf("enter pos");

    scanf("%d",&pos);

    printf("enter ele");

   scanf("%d",&ele);

delete(arr,pos,n);

    printf("after deletion");

  for(I=0;I<n-1;I++)

printf("%d",arr[I]);



}

void delete(int arr[], int pos, int size)

{

int I;

I=pos;

while(I!=size-1)

{

arr[I]=arr[I+1];

I++;

}

}

